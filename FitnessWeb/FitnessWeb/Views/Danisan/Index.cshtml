


@using FitnessWeb.Models.Siniflar;
@model List<Danisanlar>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminPanel.cshtml";
}

<br />

<table id="danisantbl" class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>Id</th>
            <th>İsim Soyisim</th>
            <th>Mail Adresi</th>

            <th>Doğum Tarihi</th>
            <th>Telefon Numarası</th>
            <th>Görsel</th>
            <th>İşlemler</th>


        </tr>
    </thead>
    @foreach (var a in Model)
    {
        <tr>
            <td>@a.DanisanId</td>
            <td>@a.DanisanIsım</td>

            <td>@a.DabisanMailAdresi</td>

            <td>@a.DogumTarih</td>
            <td>@a.TelefonNo</td>
            <td><img alt="" style="width:80px; align-items:inherit ;" title="" class="img-circle img-thumbnail isTooltip" src="@a.GörselUrl" data-original-title="Usuario"></td>


            <td><a id="" href="/Danisan/Sil/@a.DanisanId" class="btn btn-danger">Sil</a> <br /> <br /><a href="/Danisan/Guncelle/@a.DanisanId" class="btn btn-warning">Güncelle</a></td>

        </tr>
    }
</table>

<a href="/Danisan/Ekle/" class="btn btn-success">Ekle</a>
<link href="~/Content/DataTables/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/DataTables/css/dataTables.bootstrap4.css" rel="stylesheet" />
<link href="https://cdn.datatables.net/1.13.7/js/jquery.dataTables.min.js" rel="stylesheet" />
<script type="text/javascript" src="https://cdn.datatables.net/1.13.7/js/jquery.dataTables.min.js"></script>
<link href="~/Content/DataTables/css/dataTables.material.min.css" rel="stylesheet" />
<link href="~/Content/DataTables/css/dataTables.jqueryui.min.css" rel="stylesheet" />
<script>

    $("#danisantbl").DataTable();

</script>


