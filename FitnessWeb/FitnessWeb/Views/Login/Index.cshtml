<!DOCTYPE html>
<html lang="en">
<head>
    <title>Spor Dünyam</title>
    <!-- custom-theme -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Project Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //custom-theme -->
    <!-- Google fonts -->
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext" rel="stylesheet">
    <!-- Google fonts -->
    <!-- css files -->
    <link href="~/weblogin/css/style.css" type="text/css" rel="stylesheet" media="all">
    <link rel="icon" href="~/weblogin/images/logo2.png" type="image/x-icon" />
    <!-- //css files -->

    <link rel="stylesheet" href="~/weblogin/css/font-awesome.css"> <!-- Font-Awesome-Icons-CSS -->

    <link href="~/weblogin/css/popup-box.css" rel="stylesheet" type="text/css" media="all" /> <!-- popup box css -->

</head>
<!-- body starts -->
<body>

    <!-- section -->
    <section class="register">
        <div class="header">
            <div class="logo">
                <a href="#">Spor Sağlıklı Yaşam</a>
            </div>
           
            <div class="clear"></div>
        </div>

        <div class="register-full">

            <div class="register-right">
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialogk">Kayıt Ol »</a>
                </div>
                <div class="register-in middle">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog">Kullanıcı Girişi  »</a>
                </div>
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog1">Antrenör Girişi  »</a>
                </div>
                <div class="register-in middle">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog2">Admin  »</a>
                </div>
                <div>
                    <br>
                    <br />

                </div>

                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
        <!-- copyright -->
        <p </p>
        <!-- //copyright -->
    </section>
    <!-- //section -->
    <!-- login form popup-->
    <div class="pop-up">
        <div id="small-dialogk" class="mfp-hide book-form">
            @Html.Action("ParitalKayit", "Login")
        </div>


    </div>
    <div class="pop-up">
        <div id="small-dialog1" class="mfp-hide book-form">

            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Antrenör Olarak Giriş Yap</h3>
                    <span class="fa fa-lock"></span>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("AntLogin", "AntenörLogin", FormMethod.Post))
                        {
                            <input type="text" class="name" name="AntMailAdresi" Placeholder="E-mail Adresi" required="" />
                            <input type="password" class="password" name="AntSifre" Placeholder="Şifre" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                    <div class="login-agileits-bottom">
                        <h6><a href="/DanisanPanel/SifreReset">Şifremi Unuttum?</a></h6>
                    </div>

                </div>
            </div>

        </div>
    </div>
    <div class="pop-up">
        <div id="small-dialog2" class="mfp-hide book-form">

            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Admin Olarak Giriş Yap</h3>
                    <span class="fa fa-lock"></span>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("Adminlogin", "AdminPanel", FormMethod.Post))
                        {
                            <input type="text" class="name" name="AdminMailAdresi" Placeholder="E-mail Adresi" required="" />
                            <input type="password" class="password" name="AdminSifre" Placeholder="Şifre" required="" />
                            <input type="submit" value="Login">
                        }
                    </div>
                    <div class="login-agileits-bottom">
                        <h6><a href="/DanisanPanel/SifreReset">Şifremi Unuttum?</a></h6>
                    </div>

                </div>
            </div>

        </div>
    </div>
    <!-- //login form popup-->
    <!-- subscribe form popup-->
    <div class="pop-up">
        <div id="small-dialog1" class="mfp-hide book-form">

            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Be the first to hear</h3>
                    <p>Sign up to get notified about latest offers, news and events. Stay tuned!</p>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        <form action="#" method="post">
                            <input type="text" class="name" name="name" Placeholder="Full Name" required="" />
                            <input type="email" class="email" name="email" Placeholder="Email" required="" />
                            <input type="submit" value="Get Notified">
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <!-- //subscribe form popup-->
    <!-- register form popup-->
    <div class="pop-up">
        <div id="small-dialogk" class="mfp-hide book-form">


            <div id="small-dialog" class="mfp-hide book-form">

                <div class="login-form login-form-left">
                    <div class="agile-row">
                        <h3>Kullanıcı olarak Giriş Yap</h3>
                        <span class="fa fa-lock"></span>
                        <div class="clear"></div>
                        <div class="login-agileits-top">

                            @using (Html.BeginForm("CariLogin1", "Login", FormMethod.Post))
                            {
                                <input type="text" class="name" name="DabisanMailAdresi" Placeholder="E-mail Adresi" required="" />
                                <input type="password" class="password" name="DanisanSifre" Placeholder="Şifre" required="" />
                                <input type="submit" value="Login">
                            }
                        </div>
                        <div class="login-agileits-bottom">
                            <h6><a href="/DanisanPanel/SifreReset">Şifremi Unuttum?</a></h6>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>
    <!-- //register form popup-->
    <!-- js -->
    <script type="text/javascript" src="~/weblogin/js/jquery-2.1.4.min.js"></script>
    <!-- //js -->
    <!--popup-js-->
    <script src="~/weblogin/js/jquery.magnific-popup.js" type="text/javascript"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.popup-with-zoom-anim').magnificPopup({
                type: 'inline',
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'my-mfp-zoom-in'
            });

        });
    </script>
    <!--//popup-js-->

</body>
<!-- //body ends -->
</html>